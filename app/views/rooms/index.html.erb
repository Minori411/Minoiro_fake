
<div class="back_white p-t3 dashContent">
  <div class="dashContent_right">
      <div class="flex flexbox-justify-content-space-between flexbox-align-center">
          <h1>メッセージ</h1>
      </div>
      <% @another_entries.each do |e| %>
        <%= link_to room_path(e.room.id) do %>
      <div class="memberMsgBox reminder_setting">
        <div class="messages-username"><%= e.user.name %></div><br>
        <div class="messages-year"><%= e.created_at.strftime("%Y-%m-%d %H:%M") %></div><br>
        <%= Message.find_by(id: e.room.message_ids.last).body %>
      </div>
      <% end %>
      <% end %>
  </div>
</div>
