<% provide(:title, 'All users') %>
<h1>All users</h1>

<h1>ユーザー検索</h1>
<%= search_form_for @q, url: search_users_path do |f| %>
  <%= f.label :name_cont, 'ユーザー名' %>
  <%= f.search_field :name_cont %>
  <br>
  <% f.submit '検索' %>
<% end %>  

<ul class="users">
  <% User.all.each do |user| %>
    <%= user.name %>
    <%= user.email %>
    <%= user.password %>
    <%= user.password_confirmation %>
  <% end %>
</ul>