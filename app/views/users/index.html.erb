<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= search_form_for @q, url: search_users_path do |f| %>
  <%= f.search_field :name_cont,:placeholder => "検索" %>
  <br>
  <% f.submit '検索' %>
<% end %>

<ul class="users">
  <% User.all.each do |user| %>
    <%= user.name %>
    <%= user.email %>
    <%= user.password %>
    <%= user.password_confirmation %>
  <% end %>
</ul>